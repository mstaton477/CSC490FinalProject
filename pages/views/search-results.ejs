<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Search Results</title>
</head>
<body>
    <% data.books.forEach(function(key) { %>
        <li><%= key.title %></li>
        <ul>
            <% key.authors.forEach(function(a) { %>
                <li><%= a.name %></li>
            <% }) %>
        </ul>
    <% }) %>
</body>
</html>